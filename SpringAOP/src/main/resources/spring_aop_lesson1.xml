<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.1.xsd
    http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-4.1.xsd ">
   
   <bean id="circle" class="org.gcp.spring.aop.lesson1.Circle">
      <property name="name" value="Circle"/>
   </bean>
   
   <bean id="triangle" class="org.gcp.spring.aop.lesson1.Triangle">
     <property name="name" value="Triangle"/>
   </bean>
   
   <bean id="shapeService" class="org.gcp.spring.aop.lesson1.ShapeService" autowire="byName"></bean>

</beans>